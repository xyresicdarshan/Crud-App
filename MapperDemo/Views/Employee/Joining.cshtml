
@{
    ViewBag.Title = "Joining";
}
@section HeadContent{
    @Styles.Render("~/Content/Employee")
    @Styles.Render("~/Content/css")
}

<div ng-app="EmployeeApp">
    <div ng-controller="JoiningCtrl as jc">
        <h2>Joining</h2>
        <div>
            <h1 class="txthead">LINQ GroupJoin</h1>


            <table>
                <tr ng-repeat="data in jc.joinlist">
                    <br />
                    <th>
                        Department Name:  {{data.DepartmentName}}<br />
                        Department Lead: {{data.DepartmentLead}}<br />
                        Lead Email: {{data.DeptLeadEmail}}
                        <br />
                        <br />
                        <table>
                            <tr style="text-align:center">
                                <th>Employee Id</th>
                                <th>Employee Name</th>
                                <th>EmpLoyee Email</th>
                                <th>Designation</th>
                                <th>T&S</th>
                                <th>WFH</th>
                            </tr>
                            <tr ng-repeat="emp in data.Employees">
                                <td>{{emp.EmployeeId}}</td>
                                <td>{{emp.EmployeeName}}</td>
                                <td>{{emp.EmployeeEmail}}</td>
                                <td>{{emp.Designation}}</td>
                                <td><md-checkbox ng-model="emp.Tos" ng-disabled="true"></md-checkbox></td>
                                <td><md-checkbox ng-model="emp.Wfh" ng-disabled="true"></md-checkbox></td>
                                <!--<td><md-button class="textAR md-raised md-primary" ng-click="jc.ViewEmployee(data.id, data.flag)"> View </md-button></td>
    <td><md-button class="textAR md-raised" ng-click="jc.UpdateEmployee(data.id, data.flag)"> Update</md-button></td>
    <td><md-button type="submit" class="textAR md-raised md-warn" ng-click="jc.DeleteEmployee(data.id)">Delete</md-button></td>-->
                            </tr>
                        </table>

                    </th>

                </tr>

                </tr>
            </table>

        </div>
    </div>
    @section scripts{
        @Scripts.Render("~/bundles/Angular")
        @Scripts.Render("~/bundles/Common")
        @Scripts.Render("~/bundles/Joining")

    }
</div>


